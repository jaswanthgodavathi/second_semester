    <function name="GeneralCases" type="None" variable="">
        <parameters>
            <parameter name="Arraydes" type="String" array="True"/>
            <parameter name="Arrayob" type="Integer" array="True"/>
            <parameter name="Arrayval" type="Integer" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="b, rods, colds, ids" type="Integer" array="False" size=""/>
            <assign variable="rods" expression="Arrayval[2]"/>
            <assign variable="colds" expression="Arrayval[1]"/>
            <assign variable="ids" expression="Arrayval[0]"/>
            <assign variable="b" expression="0"/>
            <comment text="initial point blocked right and below"/>
            <if expression="arraydes[1]==&quot;00&quot;&amp;&amp;arraydes[n]==&quot;00&quot;">
                <then>
                    <output expression="&quot;SORRY YOU HAVE BEEN BLOCKED BY OBSTACLES&quot;" newline="True"/>
                    <assign variable="b" expression="1"/>
                </then>
                <else>
                    <comment text="botttom right corner"/>
                    <if expression="arraydes[ids]==arraydes[n*n-1]">
                        <then>
                            <if expression="arraydes[ids-1]==&quot;00&quot;&amp;&amp;arraydes[ids-n]==&quot;00&quot;">
                                <then>
                                    <output expression="&quot;SORRY YOU HAVE BEEN BLOCKED BY OBSTACLES&quot;" newline="True"/>
                                    <assign variable="b" expression="1"/>
                                </then>
                                <else/>
                            </if>
                        </then>
                        <else>
                            <comment text="last row blocked by triangular elements around it"/>
                            <if expression="ids&gt;(n*n)-n+1&amp;&amp;ids&lt;n*n-1">
                                <then>
                                    <if expression="arraydes[ids-1]==&quot;00&quot;&amp;&amp;arraydes[ids-n]==&quot;00&quot;&amp;&amp;arraydes[ids+1]==&quot;00&quot;">
                                        <then>
                                            <output expression="&quot;SORRY YOU HAVE BEEN BLOCKED BY OBSTACLES&quot;" newline="True"/>
                                            <assign variable="b" expression="1"/>
                                        </then>
                                        <else/>
                                    </if>
                                </then>
                                <else>
                                    <comment text="first row blocked by triangular elements around it"/>
                                    <if expression="ids&lt;n-1&amp;&amp;ids&gt;0">
                                        <then>
                                            <if expression="arraydes[ids-1]==&quot;00&quot;&amp;&amp;arraydes[ids+n]==&quot;00&quot;&amp;&amp;arraydes[ids+1]==&quot;00&quot;">
                                                <then>
                                                    <output expression="&quot;SORRY YOU HAVE BEEN BLOCKED BY OBSTACLES&quot;" newline="True"/>
                                                    <assign variable="b" expression="1"/>
                                                </then>
                                                <else/>
                                            </if>
                                        </then>
                                        <else>
                                            <comment text="first column blocked by triangular elements around it"/>
                                            <if expression="colds==1&amp;&amp;ids&gt;0&amp;&amp;ids&lt;(n*n)-n">
                                                <then>
                                                    <if expression="arraydes[ids+1]==&quot;00&quot;&amp;&amp;arraydes[ids-n]==&quot;00&quot;&amp;&amp;arraydes[ids+n]==&quot;00&quot;">
                                                        <then>
                                                            <output expression="&quot;SORRY YOU HAVE BEEN BLOCKED BY OBSTACLES&quot;" newline="True"/>
                                                            <assign variable="b" expression="1"/>
                                                        </then>
                                                        <else/>
                                                    </if>
                                                </then>
                                                <else>
                                                    <comment text="last column blocked by triangular elements around it"/>
                                                    <if expression="colds==n&amp;&amp;ids&gt;n&amp;&amp;ids&lt;(n*n)-1">
                                                        <then>
                                                            <if expression="arraydes[ids-1]==&quot;00&quot;&amp;&amp;arraydes[ids-n]==&quot;00&quot;&amp;&amp;arraydes[ids+n]==&quot;00&quot;">
                                                                <then>
                                                                    <output expression="&quot;SORRY YOU HAVE BEEN BLOCKED BY OBSTACLES&quot;" newline="True"/>
                                                                    <assign variable="b" expression="1"/>
                                                                </then>
                                                                <else/>
                                                            </if>
                                                        </then>
                                                        <else>
                                                            <comment text="top corner"/>
                                                            <if expression="colds==n&amp;&amp;ids==n-1">
                                                                <then>
                                                                    <if expression="arraydes[ids-1]==&quot;00&quot;&amp;&amp;arraydes[ids+n]==&quot;00&quot;">
                                                                        <then>
                                                                            <output expression="&quot;SORRY YOU HAVE BEEN BLOCKED BY OBSTACLES&quot;" newline="True"/>
                                                                            <assign variable="b" expression="1"/>
                                                                        </then>
                                                                        <else/>
                                                                    </if>
                                                                </then>
                                                                <else>
                                                                    <comment text="bottom left corner"/>
                                                                    <if expression="rods==n&amp;&amp;ids==(n*n)-n">
                                                                        <then>
                                                                            <if expression="arraydes[ids+1]==&quot;00&quot;&amp;&amp;arraydes[ids-n]==&quot;00&quot;">
                                                                                <then>
                                                                                    <output expression="&quot;SORRY YOU HAVE BEEN BLOCKED BY OBSTACLES&quot;" newline="True"/>
                                                                                    <assign variable="b" expression="1"/>
                                                                                </then>
                                                                                <else/>
                                                                            </if>
                                                                        </then>
                                                                        <else>
                                                                            <comment text="middle"/>
                                                                            <if expression="rods&gt;1&amp;&amp;rods&lt;n&amp;&amp;colds&gt;1&amp;&amp;colds&lt;n">
                                                                                <then>
                                                                                    <if expression="arraydes[ids+1]==&quot;00&quot;&amp;&amp;arraydes[ids-1]==&quot;00&quot;&amp;&amp;arraydes[ids+n]==&quot;00&quot;&amp;&amp;arraydes[ids-n]==&quot;00&quot;">
                                                                                        <then>
                                                                                            <output expression="&quot;SORRY YOU HAVE BEEN BLOCKED BY OBSTACLES&quot;" newline="True"/>
                                                                                            <assign variable="b" expression="1"/>
                                                                                        </then>
                                                                                        <else/>
                                                                                    </if>
                                                                                </then>
                                                                                <else/>
                                                                            </if>
                                                                        </else>
                                                                    </if>
                                                                </else>
                                                            </if>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
            <assign variable="arrayval[3]" expression="b"/>
        </body>
    </function>
    <function name="Path" type="None" variable="">
        <parameters>
            <parameter name="arrayval" type="Integer" array="True"/>
            <parameter name="n" type="Integer" array="False"/>
            <parameter name="arraydes" type="String" array="True"/>
            <parameter name="obs" type="Integer" array="False"/>
        </parameters>
        <body>
            <output expression="&quot;PLEASE WAIT WHILE PROCESSING A SUITABLE PATH&quot;" newline="True"/>
            <declare name="rd1, cd1, rd2, cd2, rods, colds, ids, c, loop, loop2" type="Integer" array="False" size=""/>
            <declare name="t" type="Integer" array="False" size=""/>
